<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/circle_out">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="?attr/tool_theme_text_color" />
            <solid android:color="?attr/tool_main_bg_color" />
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="oval">
            <solid android:color="?attr/tool_theme_text_color" />
        </shape>
    </item>

</layer-list>